p from \"../../http-common\";\r\n\r\n    export default {\r\n        name: \"AddOutAddress\",\r\n        data() {\r\n            return {\r\n                outaddress: {\r\n                    out_id: \"\",\r\n                    outName: \"\",\r\n                    out_mobile:\"\",\r\n                    out_email: \"\",\r\n                    outCompany:\"\",\r\n                    out_dep_phone: \"\"\r\n\r\n                },\r\n                added: false\r\n            }\r\n        },\r\n        methods: {\r\n            addOut() {\r\n                var data = {\r\n                    out_id: this.outaddress.out_id,\r\n                    outName: this.outaddress.outName,\r\n                    out_mobile: this.outaddress.out_mobile,\r\n                    out_email: this.outaddress.out_email,\r\n                    outCompany: this.outaddress. outCompany,\r\n                    out_dep_phone: this.outaddress.out_dep_phone\r\n                };\r\n                http\r\n                    .post(\"/outaddress/outaddress/add\", data)\r\n                    .then(response => {\r\n                        this.outaddress.out_id = response.data.out_id\r\n                        console.log(response.data);\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e);\r\n                    });\r\n                this.added = true;\r\n            },\r\n            newOutAddress() {\r\n                this.added = false;\r\n                this.outaddress={};\r\n            },\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/managerpage"}]}