A,KAAA,CAAA,MAAA,IAAA,EAAA,IAAA,KAAA,KAAA,CAAA,MAAA,IAAA,EAAA,EAAA;AAAA;AACA,YAAA,IAAA,GAAA;AACA,UAAA,MAAA,EAAA,KAAA,KAAA,CAAA,MADA;AAEA,UAAA,MAAA,EAAA,KAAA,KAAA,CAAA;AAFA,SAAA;;AAMA,YAAA,KAAA,KAAA,CAAA,MAAA,KAAA,OAAA,IAAA,KAAA,KAAA,CAAA,MAAA,KAAA,MAAA,EAAA;AACA,eAAA,SAAA,GAAA,IAAA;AACA,UAAA,cAAA,CAAA,OAAA,CAAA,WAAA,EAAA,KAAA,SAAA;AACA,UAAA,cAAA,CAAA,OAAA,CAAA,UAAA,EAAA,KAAA,SAAA,CAAA,OAAA,EAHA,CAGA;;AACA,eAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,CAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,GADA,CACA;AACA,WANA,CAQA;;AACA,SATA,MASA;AAEA;AACA,UAAA,IAAA,CACA,IADA,CACA,QADA,EACA,IADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AAEA,gBAAA,QAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AAAA;AACA,cAAA,KAAA,CAAA,SAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CADA,CACA;;AACA,cAAA,KAAA,CAAA,SAAA,CAAA,WAAA,GAAA,IAAA,CAFA,CAEA;;AAEA,cAAA,cAAA,CAAA,OAAA,CAAA,UAAA,EAAA,KAAA,CAAA,SAAA,CAAA,OAAA,EAJA,CAIA;;AACA,cAAA,cAAA,CAAA,OAAA,CAAA,cAAA,EAAA,KAAA,CAAA,SAAA,CAAA,WAAA,EALA,CAKA;;AAGA,mBAAA,IAAA,EAAA,GAAA,CAAA,EAAA,EAAA,GAAA,CAAA,EAAA,EAAA,EAAA,EAAA;AACA,gBAAA,QAAA,CAAA,MAAA,GADA,CACA;AACA;AAEA,aAZA,MAYA;AACA,cAAA,KAAA,CAAA,2BAAA,CAAA;AACA;AAEA,WApBA,EAqBA,KArBA,CAqBA,UAAA,CAAA,EAAA;AACA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAxBA;AAyBA;AAEA,OA9CA,MA8CA;AAAA;AACA,QAAA,KAAA,CAAA,cAAA,CAAA;AACA,OAjDA,CAiDA;;AACA,KAnDA,CAmDA;;AAnDA,GAxBA;AA6EA;AACA,EAAA,OA9EA,qBA8EA;AACA,QAAA,cAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA;AACA,WAAA,SAAA,CAAA,WAAA,GAAA,cAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,WAAA,SAAA,GAAA,cAAA,CAAA,OAAA,CAAA,WAAA,CAAA;AACA;AACA;AApFA,CAAA","sourcesContent":["<template>\r\n    <div class=\"mainForm\">\r\n        <!--로그인이 된 경우 보일 <div> 태그 부분-->\r\n        <div v-if=\"!loginInfo.loginStatus && !isManager\">\r\n            <div class=\"form-group\">\r\n                <label class=\"col-form-label col-form-label-lg inputLbl\" for=\"emp_id\">사원번호</label>\r\n                <input class=\"form-control form-control-lg\" type=\"text\" id=\"emp_id\" v-on:keyup.enter=\"login\" required\r\n                       v-model=\"input.emp_id\" placeholder=\"사원번호를 입력해주세요\">\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n                <label class=\"col-form-label col-form-label-lg inputLbl\" for=\"emp_pw\">비밀번호</label>\r\n                <input class=\"form-control form-control-lg\" type=\"password\" v-on:keyup.enter=\"login\" id=\"emp_pw\"\r\n                       required v-model=\"input.emp_pw\" placeholder=\"비밀번호를 입력해주세요\">\r\n            </div>\r\n            <br>\r\n\r\n            <button v-on:click=\"login()\" class=\"btn btn-primary btn-lg loginBtn\">Login</button>\r\n        </div>\r\n\r\n        <div v-else-if=\"!isManager\">\r\n            <myInfo></myInfo>\r\n            <working></working>\r\n            <sign></sign>\r\n            <calendar></calendar>\r\n            <today></today>\r\n            <!--ex) {{this.$router.push(\"/mypage\")}}-->\r\n            <!--요약 메인 페이지를 하나 만들고 위처럼 주소 push 해서 넘기면 된다.(로그인 성공 시)-->\r\n        </div>\r\n\r\n        <div v-else-if=\"isManager\">\r\n            <h4>You logIned successfully!</h4>\r\n            매니저 로그인 되었을 때 메인\r\n            ex) {{this.$router.push(\"/manager/employees\")}}\r\n            <!--요약 메인 페이지를 하나 만들고 위처럼 주소 push 해서 넘기면 된다.(로그인 성공 시)-->\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import http from \"../http-common\";\r\n    import working from \"./main/Working\";\r\n    import calendar from \"./main/MainCalendar\";\r\n    import myInfo from \"./main/MyInfo\";\r\n    import sign from \"./main/SignSummary\";\r\n    import today from \"./main/TodaySchedule\";\r\n\r\n    export default {\r\n        name: \"employee-login\",\r\n        data() {\r\n            return {\r\n                input: {\r\n                    emp_id: \"\",    // 사원번호 입력 값 검사를 위한 변수\r\n                    emp_pw: \"\",     // 비밀번호 입력 값 검사를 위한 변수\r\n                    manager_id: \"\",\r\n                    manager_pw: \"\"\r\n                },\r\n                loginInfo: {\r\n                    loginStatus: \"\", //로그인 상태 담을 변수\r\n                    loginId: \"\"      // 로그인 아이디\r\n                },\r\n                isManager : \"\"   // 매니저 로그인 판별할 변수\r\n            };\r\n        },\r\n        components: {\r\n            myInfo:myInfo,\r\n            working:working,\r\n            sign:sign,\r\n            calendar:calendar,\r\n            today:today\r\n        },\r\n        methods: {\r\n            login() {\r\n                if (this.input.emp_id != \"\" && this.input.emp_pw != \"\") { // 사원번호, 비밀번호 input 빈 칸 확인\r\n                    var data = {\r\n                        emp_id: this.input.emp_id,\r\n                        emp_pw: this.input.emp_pw,\r\n\r\n                    };\r\n\r\n                    if((this.input.emp_id === \"10000\") && (this.input.emp_pw===\"0000\")){\r\n                        this.isManager = true;\r\n                        sessionStorage.setItem(\"isManager\", this.isManager);\r\n                        sessionStorage.setItem(\"login_id\", this.loginInfo.loginId);           // sessionStorage 에 로그인 아이디(사번) 저장하기\r\n                        for (let i = 0; i < 1; i++) {\r\n                            location.reload();// 로그인 시 한 번만 새로고침\r\n                        }\r\n\r\n                        // sessionStorage.setItem(\"login_status\", this.loginInfo.loginStatus);   // sessionStorage 에 로그인 상태 저장하기\r\n                    } else{\r\n\r\n                    // UpfficeBack 의 LoginController 로 매핑\r\n                    http\r\n                        .post(\"/login\", data)\r\n                        .then(response => {\r\n\r\n                            if (response.data != 0) {  // 결과 값이 0이 아니면 DB에 사번(아이디), 비밀번호가 존재한다는 뜻\r\n                                this.loginInfo.loginId = response.data;   // loginId에 응답 데이터를 넣는다.(응답 데이터는 사원번호)\r\n                                this.loginInfo.loginStatus = true;        // loginStatus 에 현재 로그인 상태 true로 바꾸기\r\n\r\n                                sessionStorage.setItem(\"login_id\", this.loginInfo.loginId);           // sessionStorage 에 로그인 아이디(사번) 저장하기\r\n                                sessionStorage.setItem(\"login_status\", this.loginInfo.loginStatus);   // sessionStorage 에 로그인 상태 저장하기\r\n\r\n\r\n                                for (let i = 0; i < 1; i++) {\r\n                                    location.reload();// 로그인 시 한 번만 새로고침\r\n                                }\r\n\r\n                            } else {\r\n                                alert(\"사원 번호 또는 비밀번호가 일치하지 않습니다!\");\r\n                            }\r\n\r\n                        })\r\n                        .catch(e => {\r\n                            /* eslint-disable no-console */\r\n                            console.log(e);\r\n                        });\r\n                    }\r\n\r\n                } else { // 사원번호, 비밀번호 input 빈 칸인 경우\r\n                    alert(\"빈 칸을 확인해주세요!\");\r\n                } // End : if-else\r\n            } // End - login()\r\n\r\n        }, // End - methods\r\n        mounted() {\r\n            if (sessionStorage.length > 0) {\r\n                // 로그인 상태를 <div> 태그에서 판별하기 위해 this.loginInfo.loginStatus 로 대입한다.\r\n                this.loginInfo.loginStatus = sessionStorage.getItem(\"login_status\");\r\n                this.isManager = sessionStorage.getItem(\"isManager\");\r\n            }\r\n        }\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .mainForm {\r\n        max-width: 300px;\r\n        margin: 100px auto auto;\r\n    }\r\n\r\n    .inputLbl {\r\n        margin-right: 220px;\r\n        font-weight: bold;\r\n    }\r\n\r\n    .loginBtn {\r\n        width: 300px;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components"}]}