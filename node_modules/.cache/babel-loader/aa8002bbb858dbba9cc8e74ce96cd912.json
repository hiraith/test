,yBAqBA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA;AACA;;AAzBA;AAhBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"submitform\">\r\n        <subMenu></subMenu>\r\n        <div v-if=\"!submitted\">\r\n            <div class=\"form-group\">\r\n                <label for=\"name\">Name</label>\r\n                <input type=\"text\" class=\"form-control\" id=\"name\" required v-model=\"customer.name\" name=\"name\">\r\n            </div>\r\n\r\n            <div class=\"form-group\">\r\n                <label for=\"age\">Age</label>\r\n                <input type=\"number\" class=\"form-control\" id=\"age\" required v-model=\"customer.age\" name=\"age\">\r\n            </div>\r\n\r\n            <button v-on:click=\"saveCustomer\" class=\"btn btn-success\">Submit</button>\r\n        </div>\r\n\r\n        <div v-else>\r\n            <h4>You submitted successfully!</h4>\r\n            <button class=\"btn btn-success\" v-on:click=\"newCustomer\">Add</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import http from \"../../http-common\";\r\n    import CustomerSubMenu from \"./CustomerSubMenu\";\r\n\r\n    export default {\r\n        name: \"add-customer\",\r\n        data() {\r\n            return {\r\n                customer: {\r\n                    id: 0,\r\n                    name: \"\",\r\n                    age: 0,\r\n                    active: false\r\n                },\r\n                submitted: false\r\n            };\r\n        },\r\n        components: {\r\n            subMenu: CustomerSubMenu\r\n        },\r\n        methods: {\r\n            /* eslint-disable no-console */\r\n            saveCustomer() {\r\n                var data = {\r\n                    name: this.customer.name,\r\n                    age: this.customer.age\r\n                };\r\n\r\n                http\r\n                    .post(\"/customer/customer\", data)\r\n                    .then(response => {\r\n                        this.customer.id = response.data.id;\r\n                        console.log(response.data);\r\n\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e);\r\n                    });\r\n\r\n                this.submitted = true;\r\n            },\r\n            newCustomer() {\r\n                this.submitted = false;\r\n                this.customer = {};\r\n            }\r\n            /* eslint-enable no-console */\r\n        }\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .submitform {\r\n        max-width: 300px;\r\n        margin: auto;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src/components/customer"}]}