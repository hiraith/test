esponse => {\r\n                        this.customer.active = response.data.active;\r\n                        console.log(response.data);\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e);\r\n                    });\r\n            },\r\n            deleteCustomer() {\r\n                http\r\n                    .delete(\"/customer/customer/\" + this.customer.id)\r\n                    .then(response => {\r\n                        console.log(response.data);\r\n                        this.$emit(\"refreshData\");\r\n                        this.$router.push('/');\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e);\r\n                    });\r\n            }\r\n            /* eslint-enable no-console */\r\n        }\r\n\r\n    };\r\n</script>\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/components/customer"}]}