>\r\n    <div class=\"searchform\">\r\n      <subMenu></subMenu>\r\n        <h4>Find by Age</h4>\r\n        <div class=\"form-group\">\r\n            <input type=\"number\" class=\"form-control\" id=\"age\" required v-model=\"age\" name=\"age\">\r\n        </div>\r\n\r\n        <div class=\"btn-group\">\r\n            <button v-on:click=\"searchCustomers\" class=\"btn btn-success\">Search</button>\r\n        </div>\r\n\r\n        <ul class=\"search-result\">\r\n            <li v-for=\"(customer, index) in customers\" :key=\"index\">\r\n                <h6>{{customer.name}} ({{customer.age}})</h6>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import http from \"../../http-common\";\r\n    import CustomerSubMenu from \"./CustomerSubMenu\";\r\n\r\n    export default {\r\n        name: \"search-customer\",\r\n        data() {\r\n            return {\r\n                age: 0,\r\n                customers: []\r\n            };\r\n        },\r\n        components: {\r\n            subMenu: CustomerSubMenu\r\n        },\r\n        methods: {\r\n            /* eslint-disable no-console */\r\n            searchCustomers() {\r\n                http\r\n                    .get(\"/customer/customers/age/\" + this.age)\r\n                    .then(response => {\r\n                        this.customers = response.data; // JSON are parsed automatically.\r\n                        console.log(response.data);\r\n                    })\r\n                    .catch(e => {\r\n                        console.log(e);\r\n                    });\r\n            }\r\n            /* eslint-enable no-console */\r\n        }\r\n    };\r\n</script>\r\n\r\n<style>\r\n    .searchform {\r\n        max-width: 300px;\r\n        margin: auto;\r\n    }\r\n\r\n    .search-result {\r\n        margin-top: 20px;\r\n        text-align: left;\r\n    }\r\n</style>"],"sourceRoot":"src/components/customer"}]}